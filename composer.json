{
    "name": "secit-pl/validation-bundle",
    "type": "symfony-bundle",
    "description": "Additional Symfony validators.",
    "keywords": ["symfony", "validator", "file", "file extension", "xss", "html"],
    "homepage": "https://secit.pl",
    "license": "MIT",
    "authors": [
        {
            "name": "Tomasz Gemza",
            "email": "t.gemza@secit.pl"
        }
    ],
    "require": {
        "php": ">=8.2.0",
        "symfony/framework-bundle": "~7.0",
        "symfony/dependency-injection": "~7.0",
        "symfony/expression-language": "~7.0",
        "symfony/http-foundation": "~7.0",
        "symfony/validator": "~7.0",
        "voku/anti-xss": "~4.1"
    },
    "require-dev": {
        "phpunit/phpunit": "@stable",
        "doctrine/orm": "^2.6"
    },
    "suggest": {
        "wesbos/burner-email-providers": "Needed for BurnerEmailValidator"
    },
    "autoload": {
        "psr-4": { "SecIT\\ValidationBundle\\": "" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.0-dev"
        }
    }
}
